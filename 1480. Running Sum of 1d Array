//Question Link:-https://leetcode.com/problems/running-sum-of-1d-array/
class Solution {
    public int[] runningSum(int[] nums) {
     int []sum = new int[nums.length];
     int l=0,temp=0;
     
     for (int i=0;i<nums.length-1;i++){
         l++;
         temp=0;
        nums[l]=nums[l]+nums[i];
       
         
     }
     return nums;
    }
}
