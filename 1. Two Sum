//1. Two Sum
class Solution {
    public int[] twoSum(int[] nums, int target) {
        int x=0,y=0;
        for(int i=0;i<nums.length;i++){
            for (int j=0;j<nums.length;j++){
                int r= target-nums[i];
                if(i!=j){ 
                if(r==nums[j]){
                    x=i;
                    y=j;
                    break;
                }
                }
            }   
                if (x>0){
                    break;
                }
            
            
        }
        int[] arr=new int[2];
        arr[0]=x;
        arr[1]=y;
        return arr;
    }
}
